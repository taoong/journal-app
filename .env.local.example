# Supabase (get from your Supabase project settings)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# For Obsidian import script (get from Supabase Dashboard > Settings > API)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
# Get user UUID from Supabase Dashboard > Authentication > Users
IMPORT_USER_ID=your-user-uuid

# Oura Ring Integration (get from https://cloud.ouraring.com/oauth/applications)
OURA_CLIENT_ID=your-oura-client-id
OURA_CLIENT_SECRET=your-oura-client-secret
OURA_REDIRECT_URI=http://localhost:3000/api/oauth/oura/callback
